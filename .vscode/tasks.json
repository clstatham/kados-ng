{
    // See https://code.visualstudio.com/docs/editor/tasks for more information
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Chainload over USB Serial",
            "type": "shell",
            "command": "cargo",
            "args": [
                "xtask",
                "--target=aarch64",
                "load",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$rustc"
            ]
        },
    ]
}